FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      Tr05;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 0;

boundaryField
{
	Inlet
    {
        type            zeroGradient;
    }
    Outlet
    {
        type            fixedValue;
        value           uniform 0;
    }
		
	"(Ground|Building)"
	{
		type			zeroGradient;
	}
	
	"(Paral|Roof)"
	{
		type			symmetryPlane;
	}
}

